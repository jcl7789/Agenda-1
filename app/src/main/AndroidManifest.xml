<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zeus.agenda">
    <uses-permission android:name="android.permission.WRITE_CONTACTS"></uses-permission>
    <uses-permission-sdk-23 android:name="android.permission.WRITE_CONTACTS"></uses-permission-sdk-23>
    <uses-permission android:name="android.permission.READ_CONTACTS"></uses-permission>
    <uses-permission-sdk-23 android:name="android.permission.READ_CONTACTS"></uses-permission-sdk-23>
    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
    <uses-permission-sdk-23 android:name="android.permission.CALL_PHONE"></uses-permission-sdk-23>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.ActivityAgregar" android:label="Agregar contacto"/>
        <activity
            android:name=".Activities.ActivityModificar"
            android:label="Modificar contacto" />
        <activity android:name=".Activities.ActivityListar" android:label="Lista de contactos" />
        <activity android:name=".Activities.ActivityVer" android:label="Detalle" />
    </application>

</manifest>
